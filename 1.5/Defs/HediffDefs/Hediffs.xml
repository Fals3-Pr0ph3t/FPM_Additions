<!--HeDiffs-->
<HediffDef Name="FPMResequence" Abstract="True">
    <defName>FPM_Base_Resequence</defName>
    <label>FPMBase resequencing</label>
    <description>Specially designed archites are resequencing this person's genes.</description>
    <hediffClass>HediffWithComps</hediffClass>
	<initialSeverity>1</initialSeverity>
	<maxSeverity>30</maxSeverity>
    <stages>
        <li>
			<minSeverity>1</minSeverity>
			<label>pondering</label>
            <becomeVisible>True</becomeVisible>
        </li>
		<li>
			<minSeverity>27</minSeverity>
			<label>focusing</label>
			<capMods>
				<li>
					<capacity>Consciousness</capacity>
					<setMax>0.3</setMax>
				</li>
			</capMods>
			<hungerRateFactor>0.01</hungerRateFactor>
		</li>
    </stages>
</HediffDef>

	
	
